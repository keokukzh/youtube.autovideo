{
  "name": "contentmultiplier",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm run test:ci",
    "setup": "npm install && npm run type-check && npm run format",
    "db:seed": "tsx scripts/seed-db.ts",
    "build:cf": "next build && wrangler deploy",
    "deploy:cf": "wrangler deploy",
    "dev:cf": "wrangler dev",
    "dev:worker": "wrangler dev",
    "deploy:worker": "wrangler deploy",
    "deploy:production": "npm run build && wrangler deploy --env production",
    "deploy:preview": "npm run build && wrangler deploy --env preview"
  },
